{
  "name": "veteran-status",
  "datapackage_version": "1.0-beta.18",
  "title": "Veteran Status",
  "description": "Veteran Status reports demographic details regarding the number and proportion of veterans in the population.",
  "version": "1.0",
  "licenses": [{
    "url": "http://opendatacommons.org/licenses/pddl/",
    "name": "Open Data Commons Public Domain",
    "version": "1.0",
    "id": "odc-pddl"
  }],
  "sources": [{
    "name": "uscensus",
    "web": ""
  }],
  "author":[{
    "name": "Connecticut Data Collaborative",
    "email": "info@ctdata.org",
    "web": "http://ctdata.org"
  }],
  "contributor": [],
  "resources": [{
    "path": "data/veteran-status-2017.csv",
    "format": "csv",
    "schema": {
      "fields": [{
          "name": "Town",
          "type": "string",
          "dimension": false
        }, {
          "name": "FIPS",
          "type": "string",
          "dimension": false
        }, {
          "name": "Year",
          "type": "string",
          "dimension": false
        }, {
          "name": "Age",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["18 to 34 years",
                     "35 to 54 years",
                     "55 to 64 years",
                     "65 to 74 years",
                     "75 years and over",
                     "Total"]
         }
        }, {     
          "name": "Veteran Status",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Nonveteran", 
                     "Veteran",
                     "Total"]
         }
        }, { 
          "name": "Gender",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Female", 
                     "Male",
                     "Total"]
         }
        }, { 
          "name": "Measure Type",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Number", "Percent"]
         }
        }, {
          "name": "Variable",
          "type": "string",
          "dimension": true,
          "constraints": {
            "enum": ["Veteran Status", "Margins of Error"]
         }
        }, {
          "name": "Value",
          "type": "number",
          "dimension": false
        }]
      }
    }
  ],
  "last_modified": "05/23/2019",
  "ckan_extras": {
    "full_description": {
